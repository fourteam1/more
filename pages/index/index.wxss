/**index.wxss**/


/* <!-- 轮播图 --> */


/* 拼团 */
  .assemble{
    width:100%;
    height:300rpx;
    background:red;
    margin:15rpx 0;
  }


 /* 商品列表 */
  .list{
    width:100%;
    display:flex;
    flex-wrap: wrap;
    background:#f4f4f4;
  }
  .lis{
    width:49%;
    height:540rpx;
    background:#fff;
  }
  .lis:nth-of-type(odd){
    /* background:blue; */
    margin-right:2%;
    margin-bottom:2%;
  }
  .lis_img_1{
    height:372rpx;
    width:100%;
  }
  .lis_text{
    width:100%;
    font-size:28rpx;
    color:#393939;
    height:70rpx;
    overflow:hidden;
    padding:0 10rpx;
    box-sizing: border-box;
  }
  .price_box{
    display:flex;
    width:100%;
    justify-content: space-between;
    padding:0 10rpx;
    box-sizing: border-box;
  }
  .price_1{
    height:80rpx;
    line-height:80rpx;
  }
  .price_text1{
    font-size:26rpx;
    color:#db0a00;
  }
  .price_text2{
    color:#db0a00;
    font-size:37rpx;
  }
   .price_text3{
    font-size:26rpx;
    color:#adadad;
  }

  .price_2{
    width:100rpx;
    height:80rpx;
    background:red;
  }