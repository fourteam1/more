}
/* 玲娟中心 */
.coupon_list{
  display:flex;
  justify-content: space-between;
  width:100%;
  height:343rpx;
  padding:0 24rpx;
  box-sizing: border-box;
}
.coupon_list image{
  width:164rpx;
